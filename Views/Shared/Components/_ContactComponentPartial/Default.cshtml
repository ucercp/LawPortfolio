<section id="contact" class="s-contact target-section">

        <div class="overlay"></div>
        <div class="row narrow section-intro">
            <div class="col-full">
                <h3>İletişim</h3>
                <h1>@ViewBag.contactTitle</h1>
            <p class="lead">@ViewBag.contactDescription</p>
            </div>
        </div>
        <div class="row contact__main">
        @await Component.InvokeAsync("_FormContactComponentPartial")
        <div class="col-four tab-full contact__infos">
                <h4 class="h06">Telefon</h4>
                <p>
                @ViewBag.contactPhone1
                    <br />
                @ViewBag.contactPhone2
                </p>
                <h4 class="h06">Email</h4>
                <p>
                @ViewBag.contactEmail1<br>
                @ViewBag.contactEmail2
                </p>
                <h4 class="h06">Adres</h4>
                <p>
                @ViewBag.contactAddress
                </p>
            </div>
        </div>
  
</section>